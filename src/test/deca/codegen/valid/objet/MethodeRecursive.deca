class A{
    
    int x;

    int fact(int n){
        if (n > 0){
            if (n == 1){
                return 1;
            }
            else{
                return this.fact(n -1);
            }
        }
        else{
            return 0;
        }
    }
}
{
    A a = new A();

    print(a.fact(3));
    
}

