class Mat {
    int[][] mat;
    int n;


    int[][] getMat() {
        return this.mat;
    }


    void init(int n) {
        int i = 0;
        int j = 0;
        mat = new int[n][n];
        while (i < n) {
            j = 0;
            while (j < n) {
                mat[i][j] = 0;
                j = j + 1;
            }
            i = i + 1;
        }
        this.n = n;
    }

    int get(int i, int j) {
        return mat[i][j];
    }

    void set(int i, int j, int a) {
        mat[i][j] = a;
    }

    void printMat() {
        int i = 0;
        int j = 0;
        while (i < n) {
            j = 0;
            while (j < n) {
                print(mat[i][j], " ");
                j = j + 1;
            }
            println();
            i = i + 1;
        }
    }

    void initCopy(int[][] mat2, int n) {
        int i = 0;
        int j = 0;
        mat = new int[n][n];
        while (i < n) {
            j = 0;
            while (j < n) {
                mat[i][j] = mat2[i][j];
                j = j + 1;
           }
            i = i + 1;
        }
        this.n = n;
    }

}


{
    Mat matrice = new Mat();
    Mat matrice2 = new Mat();
    int n = 10;
    matrice.init(n);
    matrice.printMat();
    matrice.set(1, 1, 10);
    println("-------------------");
    matrice.printMat();
    matrice2.initCopy(matrice.getMat(), n);
    println("-----------");
    matrice2.printMat();
    matrice2.set(1, 2, 4);
    println("-------------------");
    matrice.printMat();
    println("-------------------");
    matrice2.printMat();
    println((matrice2.getMat())[1][2]);
    println((matrice.mat)[1][1]);

}
